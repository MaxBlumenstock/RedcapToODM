            <add-attribute value="/js/jQuery/jquery-3.5.1.min.js" type= "string"/>
            <add-attribute value="/js/jQuery/jquery-ui.min.js" type= "string"/>
            <add-attribute value="/js/jQuery/fileinput.min.js" type= "string"/>
